<manifest xmlns:android="http://schemas.android.com/apk/res/android"
package="com.weidingqiang.wanbase">

<application
    android:name=".app.AppContext"
    android:allowBackup="true"
    android:icon="@mipmap/ic_launcher"
    android:label="@string/app_name"
    android:roundIcon="@mipmap/ic_launcher_round"
    android:supportsRtl="true"
    android:theme="@style/AppTheme">
    <activity android:name=".ui.login.activity.LoginActivity"/>

    <meta-data
        android:name="design_width"
        android:value="1080" />
    <meta-data
        android:name="design_height"
        android:value="1920" />

    <activity android:name=".MainActivity">
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />
            <action android:name="android.intent.action.VIEW" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
    </activity> <!-- 初始化service -->
    <!-- 初始化service -->
    <service
        android:name="com.weidingqiang.rxqwelibrary.service.InitializeService"
        android:enabled="true"
        android:exported="false" />
    <activity android:name=".ui.help.activity.HelpActivity"/>

</application>

</manifest>
